{
  "name": "railroad/usora",
  "type": "library",
  "description": "User management system.",
  "keywords": [
    "users",
    "management",
    "musora"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Caleb Favor",
      "email": "caleb@drumeo.com",
      "homepage": "https://www.musora.com",
      "role": "Developer"
    }
  ],
  "prefer-stable": true,
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/railroadmedia/laravel-apidoc-generator"
    }
  ],
  "require": {
    "php": "^8.1",
    "laravel/framework": "^9.0",
    "mikemclin/laravel-wp-password": "~2.0.1",
    "ext-redis": "*",
    "railroad/doctrine": "2.0.*",
    "jms/serializer": "1.*",
    "railroad/permissions": "2.0.*",
    "php-open-source-saver/jwt-auth": "1.4.*",
    "spatie/laravel-fractal": "^6.0",
    "railroad/doctrine-array-hydrator": "2.0.*",
    "league/flysystem": "^3.0",
    "league/flysystem-aws-s3-v3": "^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5.10",
    "phpunit/php-code-coverage": "^9.2.10",
    "orchestra/testbench": "^7.4",
    "dms/phpunit-arraysubset-asserts": "^0.4.0"
  },
  "autoload": {
    "psr-4": {
      "Railroad\\Usora\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Railroad\\Usora\\Tests\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "kylekatarnls/update-helper": true,
      "composer/package-versions-deprecated": true
    }
  }
}
